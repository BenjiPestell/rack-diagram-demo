graph "Visual Subnet Ethernet" {

  graph [
    rankdir=TB,
    nodesep=1.0,
    ranksep=1.5,
    bgcolor="white",
    label="Visual Subnet Ethernet",
    labelloc=t,
    fontsize=16,
    fontname="Sinkin Sans 400 Regular"
