    label="Rack 1";
    style=filled;
    color="#F5F5F5";
    fontname="Sinkin Sans 400 Regular";

    "Graphics_Switch_1" [
      label="Graphics Switch 1",
      fillcolor="#B8D6D6",
      penwidth=2.5
    ];
    "Operator_PC" [
      label="Operator PC",
      fillcolor="#E8F4F8"
    ];
    "Physics_PC" [
      label="Physics PC",
      fillcolor="#E8F4F8"
    ];
    "Spectator_IG" [
      label="Spectator IG",
      fillcolor="#E8F4F8"
    ];
    "Telemetry" [
      label="Telemetry",
      fillcolor="#E8F4F8"
    ];
    "dSPACE" [
