graph "Company Network" {

  graph [
    bgcolor="white",
    label="Company Network",
    labelloc=t,
    fontsize=14,
    fontname="Sinkin Sans 400 Regular",
    overlap=false,
    sep=0.5
  ];

  node [
    shape=box,
    style="rounded,filled",
    fontsize=10,
    fontname="Sinkin Sans 400 Regular",
    margin=0.2
  ];

  edge [
    color="#292929",
    style=solid,
    penwidth=2.0
  ];

  // Devices grouped by rack and external groups

  subgraph cluster_rack1 {
    label="Rack 1";
    style=filled;
    color="#F5F5F5";
    fontname="Sinkin Sans 400 Regular";

    "Company_Network_Switch" [
      label="Company Network Switch\n(3 conn)",
      fillcolor="#B8D6D6",
      penwidth=2.5
    ];
    "Company_Net_in" [
      label="Company Net in",
      fillcolor="#B8D6D6"
    ];
    "Operator_PC" [
      label="Operator PC",
      fillcolor="#E8F4F8"
    ];
    "Telemetry" [
      label="Telemetry",
      fillcolor="#E8F4F8"
    ];
  }

  // Connections
  Company_Network_Switch -- Operator_PC [
    color="#292929", style=solid, penwidth=2.0, label="   Ethernet", fontsize=7, fontname="Sinkin Sans 400 Regular"
  ];
  Company_Network_Switch -- Telemetry [
    color="#292929", style=solid, penwidth=2.0, label="   Ethernet", fontsize=7, fontname="Sinkin Sans 400 Regular"
  ];
  Company_Net_in -- Company_Network_Switch [
    color="#292929", style=solid, penwidth=2.0, label="   Ethernet", fontsize=7, fontname="Sinkin Sans 400 Regular"
  ];

}